{
  "name": "fifo",
  "version": "1.0.0",
  "description": "JavaScript library for interacting with localStorage safely.",
  "author": "Matthew Callis <matthew.callis@gmail.com>",
  "contributors": [
    {
      "name": "Matthew Callis",
      "email": "matthew.callis@gmail.com"
    }
  ],
  "main": "build/fifo.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/MatthewCallis/fifo"
  },
  "bugs": {
    "url": "https://github.com/MatthewCallis/fifo/issues"
  },
  "keywords": [
    "localStorage",
    "fifo"
  ],
  "dependencies": {},
  "devDependencies": {
    "chai": "^3.5.0",
    "coffee-script": "^1.10.0",
    "coffeelint": "^1.15.0",
    "coveralls": "^2.11.9",
    "istanbul": "^0.4.2",
    "localStorage": "^1.0.3",
    "mocha": "^2.4.5",
    "mocha-istanbul": "^0.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "mocha-phantomjs": "^4.0.2",
    "uglify-js": "^2.6.2"
  },
  "preferGlobal": false,
  "private": false,
  "license": "MIT",
  "jam": {
    "dependencies": {}
  },
  "scripts": {
    "instrument": "istanbul instrument --output build-coverage build/",
    "coverage": "istanbul instrument --output build-coverage build/ && istanbul cover _mocha -- test/*_spec.js -R spec",
    "coverage-report": "istanbul report --root coverage lcov",
    "lint": "coffeelint src/*.coffee",
    "make": "coffee --compile --output build/ src/*",
    "make-dist": "uglifyjs build/*.js --beautify --indent 2 --verbose --output dist/fifo.js",
    "make-dist-min": "uglifyjs build/*.js --compress --mangle --verbose --output dist/fifo.min.js",
    "test-compile": "coffee --compile --output test/ test/*.coffee",
    "test": "coffee --compile --output test/ test/*.coffee && ./node_modules/mocha/bin/mocha",
    "test-phantomjs": "mocha-phantomjs --setting localToRemoteUrlAccessEnabled=true --setting webSecurityEnabled=false test/index.html --hooks ./phantom_hooks.js --timeout 25000"
  },
  "github": "https://github.com/MatthewCallis/fifo"
}
